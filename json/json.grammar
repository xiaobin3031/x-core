OBJECT_KEY_VALUE = WHITESPACE* STRING WHITESPACE* ':' VALUE
OBJECT = '{' WHITESPACE* | OBJECT_KEY_VALUE (',' OBJECT_KEY_VALUE)? '}'
ARRAY = '[' WHITESPACE* | VALUE (',' VALUE)? ']'
VALUE = WHITESPACE* (STRING | NUMBER | OBJECT | ARRAY | 'true' | 'false' | 'null') WHITESPACE*
STRING = '"' not('"'|'\'|CTRL-CHAR) | \('"' | '\' | '/' | 'b' | 'f' | 'n' | 'r' | 't' | 'uHEX{4}') '"'
CTRL-CHAR = ...
NUMBER = '-'? ('0' | DIGIT_NO_ZERO(DIGIT)?) ('.' DIGIT)? ('E'|'e' ('-'|'+')? DIGIT)?
DIGIT_NO_ZERO = 1-9
DIGIT = 0 DIGIT_NO_ZERO
WHITESPACE = ' ' | '\n' | '\r' | '\t'